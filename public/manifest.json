{
    "name": "Browser Statistic",
    "version": "0.1.0",
    "description": "Show what site your often browse",
    "manifest_version": 3,
    "author": "Jen-Chieh Shen",
    "action": {
        "default_title": "Browser Statistic",
		"default_icon": {
            "38": "logo.png",
			"19": "logo.png"
        },
		"default_popup": "index.html"
    },
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "permissions": [
        "history"
    ]
}